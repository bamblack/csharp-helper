{
	"name": "csharp-helper",
	"publisher": "bamblack",
	"displayName": "csharp-helper",
	"description": "Visual Studio Code plugin that provides tools to assist with C# development",
	"version": "0.1.0",
	"engines": {
		"vscode": "^1.36.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"workspaceContains:**/*.cs"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "csharpHelper.createNewClass",
				"title": "C# Helper: New Class"
			},
			{
				"command": "csharpHelper.createNewInterface",
				"title": "C# Helper: New Interface"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.36.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"tslint": "^5.12.1",
		"vscode-test": "^1.0.0-next.0"
	},
	"dependencies": {
		"find-up-glob": "^1.0.0"
	}
}
